{
  "name": "@marketprotocol/dApp",
  "version": "0.2.0",
  "private": true,
  "homepage": "https://dapp.marketprotocol.io/",
  "engines": {
    "node": "^8.11.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-import": "^1.7.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.16.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.5.1",
    "husky": "^0.14.3",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "less-vars-to-js": "^1.2.1",
    "lint-staged": "^7.0.5",
    "prettier": "^1.12.1",
    "react-ga": "^2.5.2",
    "sinon": "^4.2.2",
    "web3-fake-provider": "^0.1.0"
  },
  "dependencies": {
    "@marketprotocol/marketjs": "0.0.3",
    "antd": "^3.6.2",
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "bignumber.js": "^7.2.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.0.1",
    "ethereum-input-data-decoder": "0.0.12",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "human-standard-token-abi": "^1.0.2",
    "jest": "20.0.4",
    "lodash": "^4.17.10",
    "market-solidity": "^1.0.1",
    "moment": "^2.20.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "query-string": "^5.0.1",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.7",
    "react-scripts": "^1.1.4",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.4.0",
    "redux-promise-middleware": "^5.0.0",
    "redux-thunk": "^2.2.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "truffle-contract": "^3.0.3",
    "url-loader": "0.6.2",
    "webfontloader": "^1.6.28",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3",
    "xmlhttprequest": "^1.8.0",
    "zeppelin-solidity": "1.8.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "start:rinkeby": "npm run copy:rinkeby && node scripts/start.js",
    "build": "node scripts/build.js",
    "build:rinkeby":
      "npm run clean && npm run copy:rinkeby && node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "test:prod": "npm run lint && npm run build:rinkeby && npm run coverage",
    "coverage": "npm run test -- --coverage",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "lint": "eslint src/**/*.js test/**/*.js",
    "lint:fix": "eslint --fix src/**/*.js test/**/*.js",
    "precommit": "lint-staged",
    "clean": "rm -rf ./dApp/",
    "copy:rinkeby": "rm -rf ./build/ && cp -r rinkeby-build/ build/",
    "prettier-less":
      "prettier --single-quote --parser less --write \"./src/**/*.less\""
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!src/containers/*.js",
      "!src/index.js",
      "!src/registerServiceWorker.js"
    ],
    "setupFiles": ["<rootDir>/config/polyfills.js"],
    "testMatch": ["<rootDir>/test/**/?(*.)(spec|test).{js,jsx,mjs}"],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)":
        "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": ["[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js"
  },
  "babel": {
    "presets": ["react-app"]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
